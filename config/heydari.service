[Unit]
Description=heydari Gunicorn
After=syslog.target

[Service]
ExecStart=/heydari/.env/bin/gunicorn --config /heydari/config/gunicorn.conf.py
RuntimeDirectory=heydari_gunicorn
Restart=always
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target
