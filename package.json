        "vite-plugin-solid": "^2.10.2",
        "vite-tsconfig-paths": "^4.3.2",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@solidjs/router": "^0.13.5",
